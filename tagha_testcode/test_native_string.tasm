%main: {
	movi		rgamal, 0
	st4			[rbp-4], rgamal ; int length = 0;
	st4			[rbp-8], rgamal ; int i = 0;
	jmp			.L2
.L3:
	loadglobal	rsemkath, test_str
	movi		ralaf, 1
