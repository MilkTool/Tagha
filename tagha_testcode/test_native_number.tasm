%main: {
	movi rbeth, 1000000000
	bit_xor ralaf, ralaf
	
.loop:
	mov rsemkath, ralaf
	movi ralaf, 1
	syscall %AddOne
	dec rbeth
	movi rgamal, 0
	cmp rbeth, rgamal
	jz .loop
	ret
}
